b = list(map(int,input().split()))
a = b[0]
c= b[1]
x= []
for i in range(a):
    n = input().split()
    nn = list(map(int,n))
    for j in nn:
        x.append(j)
x.sort()
j = []
for i in range(a):
    v= x[0:c]
    j.append(v)
    x=x[c:]
xx= []
for i in j:
    vf = ""
    for k in i:
        k = str(k)
        vf = vf+" "+k
    xx.append(vf)

for i in xx:
    i = i.strip()
    print(i)
    
